---
import HeaderEra from "../../components/HeaderEra.astro"
import Layout from "../../layouts/Layout.astro"
import TablaCanciones from "../../sections/TablaCanciones.astro"
const era = 'speakNow'
import {eras} from "../../datas/songs.json"
const eraData = eras.find(e => e.albums === era);
if (!eraData) return null

---
<Layout title = `${era} | Swifties Eras Tour` era = {era}>
    <main class=`bg-${era}-100 py-12 px-32 space-y-6 flex flex-col gap-2.5 gap-y-0 items-start justify-start self-stretch shrink-0 relative`>
        
        <HeaderEra era={era} anterior={eraData?.anterior} siguiente={eraData?.siguiente} logo={eraData.logo}/>
        
        <TablaCanciones era={era}/>

        
    </main>
</Layout>